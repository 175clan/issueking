<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="issue.king.test.api.user">
    <select id="getUserList" resultType="issue.king.test.api.model.User">
        SELECT
			USERNAME
			, PASSWORD
			, ENABLED
        FROM
            USERS
    </select>
</mapper>